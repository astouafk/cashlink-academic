# Environment
NODE_ENV=development

# Database (NeonDB)
NEON_DB_HOST=your-neon-host.neon.tech
NEON_DB_NAME=cashlink_db
NEON_DB_USER=your_user
NEON_DB_PASSWORD=your_password

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=your_secret_key
JWT_EXPIRATION=15m
JWT_REFRESH_EXPIRATION=7d

# Providers Config
WAVE_MIN_AMOUNT=50000
WAVE_MAX_AMOUNT=5000000
WAVE_FEE_PERCENTAGE=0.01

ORANGE_MONEY_MIN_AMOUNT=10000
ORANGE_MONEY_MAX_AMOUNT=2000000
ORANGE_MONEY_FEE_PERCENTAGE=0.015

FREE_MONEY_MIN_AMOUNT=5000
FREE_MONEY_MAX_AMOUNT=1000000
FREE_MONEY_FEE_PERCENTAGE=0.02

# Interoperability
INTEROP_FEE_PERCENTAGE=0.005
INTEROP_TIMEOUT=30000
